</head>

<body class="menubar-hoverable header-fixed menubar-pin ">

    <!-- BEGIN HEADER-->

    @Html.Partial("_Header")
    <!-- END HEADER-->
    <!-- BEGIN BASE-->

    <div id="base">

        <!-- BEGIN OFFCANVAS LEFT -->
        <div class="offcanvas">
        </div><!--end .offcanvas-->
        <!-- END OFFCANVAS LEFT -->
        <!-- BEGIN CONTENT-->
        <div id="content">

            <!-- BEGIN BLANK SECTION -->
            @RenderBody()

            <!-- BEGIN BLANK SECTION -->
        </div><!--end #content-->
        <!-- END CONTENT -->
        <!-- BEGIN MENUBAR-->
      
        @if (Convert.ToInt32(Session["RoleId"]) == 1)
        {
        @Html.Partial("_Menubar")
        }
        else if (Convert.ToInt32(Session["RoleId"]) == 2)
        {
        @Html.Partial("_MenubarARCS")
        }
        else if (Convert.ToInt32(Session["RoleId"]) == 3)
        {
        @Html.Partial("_MenubarInspector")
        }
        else if (Convert.ToInt32(Session["RoleId"]) == 4)
        {
            @Html.Partial("_MenubarAdmin")
        }
        else if (Convert.ToInt32(Session["RoleId"]) == 5)
        {
            @Html.Partial("_MenubarBackLog")
        }
        else
        {
        }   
        <!--end #menubar-->
        <!-- END MENUBAR -->
        <!-- BEGIN OFFCANVAS RIGHT -->
        @Html.Partial("_OffCanvas")
        <!--end .offcanvas-->
        <!-- END OFFCANVAS RIGHT -->

    </div><!--end #base-->
    
    <script src="/content/assets/js/libs/bootstrap/bootstrap.min.js"></script>
    <script src="/Content/assets/js/libs/jquery/BootStrapTour.js"></script>
    <script src="/content/assets/js/libs/spin.js/spin.min.js"></script>
