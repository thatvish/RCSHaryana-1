<script>
    function demoFromHTML() {
        //debugger;
        var pdf = new jsPDF('p', 'pt', 'letter');
        source = $('#print-content')[0];
        specialElementHandlers = {
            '#bypassme': function (element, renderer) {
                return true
            }
        };
        margins = {
            top: 80,
            bottom: 60,
            left: 40,
            width: 550
        };
        pdf.fromHTML(
            source, // HTML string or DOM elem ref.
            margins.left, // x coord
            margins.top, { // y coord
                'width': margins.width, // max width of content on PDF
                'elementHandlers': specialElementHandlers
            },
            function (dispose) {
                pdf.save('FormG.pdf');
            }, margins
        );
    }
    function AddData() {
        validateRemark();
        var res = validateRemark();
        if (res == false)
        {
            alert("Please Select Remark First");
            return false
        }
        //debugger;
        if (document.getElementById('5r1').checked) {
            var NameOfSocietyProposed_Suitable = true;
        }
        else {
            NameOfSocietyProposed_Suitable = false
        }
        if (document.getElementById('6r1').checked) {
            var SocietyOrganizedUnderProjectScheme = true;
        }
        else {
            SocietyOrganizedUnderProjectScheme = false;
        }
        if (document.getElementById('8r1').checked) {
            var ObjectConsonanceWithCooperativePrinciples = true
        }
        else {
            ObjectConsonanceWithCooperativePrinciples = false
        }
        if (document.getElementById('9r1').checked) {
            var AdoptedModalByeLaws = true
        }
        else {
            AdoptedModalByeLaws = false
        }
        if (document.getElementById('11r1').checked) {
            var QualificationsMembership = true
        }
        else {
            QualificationsMembership = false
        }
        if (document.getElementById('12r1').checked) {
            var PromoterMembers_CommonInterest = true
        }
        else {
            PromoterMembers_CommonInterest = false
        }
        if (document.getElementById('21r1').checked) {
            var OtherCoopSocietyWithSameObjects = true
        }
        else {
            OtherCoopSocietyWithSameObjects = false
        }
        // alert(prposed_suitable);
        //var res = validate();
        var objFGM = {
            DateOfSubmittionByInspector: $('#Dateofapplication').val(),
            ModeOfApplicationReceived: $('#statesociety').val(),
            SocietyName: $('#address').val(),
            SocietyOrganizedByOwnInitiative: $('#organised').val(),
            NameOfSocietyProposed_Suitable: NameOfSocietyProposed_Suitable,
            // NameOfSocietyProposed_Suitable_Detail: $('#nameproposed').val(),
            SocietyOrganizedUnderProjectScheme: SocietyOrganizedUnderProjectScheme,
            SocietyOrganizedUnderProjectScheme_Details: $('#specificPrjct').val(),
            MainObjectsOfProposedSociety: $('#Mainobjects').val(),
            ObjectConsonanceWithCooperativePrinciples: ObjectConsonanceWithCooperativePrinciples,
            ObjectConsonanceWithCooperativePrinciples_Details: $('#consonance').val(),
            AdoptedModalByeLaws: AdoptedModalByeLaws,
            AdoptedModalByeLaws_Details: $('#formulated').val(),
            DeviationsModalByeLaws_Details: $('#deviations').val(),
            QualificationsMembership: QualificationsMembership,
            QualificationsMembership_Details: $('#promoter').val(),
            PromoterMembers_CommonInterest: PromoterMembers_CommonInterest,
            PromoterMembers_CommonInterest_Details: $('#commoninterest').val(),
            VerifiedFormB: $('#verifiedB').val(),
            CorrectnessAllRespects: $('#Uprequest').val(),
            NoOfPromoters: $('#promotermembers').val(),
            AreaOfOperation: $('#Areaoperation').val(),
            AreaPopulation: $('#population').val(),
            NumberAndPaidUpValue: $('#subscribed').val(),
            ShareInKind: $('#contributed').val(),
            ExplanationToPromoters: $('#obligations').val(),
            OtherCoopSocietyWithSameObjects: OtherCoopSocietyWithSameObjects,
            OtherCoopSocietyWithSameObjects_Details: $('#similar').val(),
            ReasonForNotJoining: $('#embraced').val(),
            AdverselyEffectOthers: $('#adversely').val(),
            ConcludingRemark: $('#recommendations').val(),
            Remarks: $('Select#Remarks').val(),
        };
       // debugger;
        //alert(objFGM.VerifiedFormB);
        $.ajax({
            url: "/Inspector/FormG",
            data: JSON.stringify(objFGM),
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (i)
            {
                if (i == 1) {
                    alert("FormG has been saved successfully")
                    $('#saveId').attr('disabled', 'disabled');

                }
                else if (i == 0) {
                    alert("Kindly select any society");
                }
                else if (i >=2) {
                    alert("Form has been successfully saved.");
                }
                // loadData();
                // alert()
                //clearTextBox();
                //$('#myModal1').modal('hide');
            },
            error: function (errormessage) {
                //alert(errormessage.responseText);
            }
        });
    }

</script>
