                                    <p>
                                        2. 
                                    </p>
                                </div>
                                <div class="col-md-10 form-group">
                                    <p class="col-lg-offset-12" align="right">
                                        Countersigned
                                     </p>
                                    <p></p>
                                    <p class="col-lg-offset-12" align="right">
                                        (President)
                                    </p>
                                    <p>
                                        
                                        Place__________________
                                    </p>
                                    <p>
                                        Date __________________
                                    </p>
                                    <p>
                                        (NOTE :- Name and full address of the witnesses should be given. Any two promoter members of the society can act as witness )
                                    </p>
                                </div>
                            </div>*@
                        </div>
                    </div>
                </div>
            </div><!--end .card -->
    </div>
</div>
<!--end .section-body -->
</section>
<div class="modal fade cus-modal-div" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title" id="myModalLabel">Receipt</h4>
            </div>
            <div class="modal-body">
                <form class="form form-validate" novalidate="novalidate" action="@Url.Action("procedurefee", "Society")" accept-charset="utf-8" method="post">
                    <div class="row">
                        <div class="card">
                            <div class="card-body">
                                    <div class="col-md-12 form-group">
                                        <input type="text" class="form-control" id="ShareMoney" name="ShareMoney" required data-rule-number="true">
                                        <label for="Mainobjects">Share Money <span style="color:red;"><strong>*</strong></span></label>
                                    </div>
                                    <br />
                                    <div class="col-md-12 form-group ">
                                        <input type="text" class="form-control" id="AdmissionFee" name="AdmissionFee" required data-rule-number="true">
                                        <label for="Mainobjects">Admission Fee<span style="color:red;"><strong>*</strong></span></label>
                                    </div>
                                    <br />
                                    <div class="col-md-12 form-group">
                                        <input type="text" class="form-control" id="Deposits" name="Deposits" data-rule-number="true">
                                        <label for="Mainobjects">Deposits</label>
                                    </div>
                                
                            </div>
                        </div>

                        <div class="modal-footer">
                            <button type="submit" class="btn btn-primary" id="btnAdd">Add</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
