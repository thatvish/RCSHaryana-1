                                        <label for="district">1. Select District</label>
                                    </div>
                                    <div class="col-md-6 form-group">
                                        @Html.DropDownList("ARCSOffice", new SelectList(Enumerable.Empty<SelectListItem>(), "ACRSCode", "ACRSName"),
                                        "Select", new { id = "ARCSOffice", @class = "form-control", required = "required" })
                                        <label for="ARCSOffice">2. ARCS Office</label>
                                    </div>
                                    <div class="col-md-12 form-group">
                                        <input type="text" class="form-control" id="NameofProposedSociety" name="NameofProposedSociety" required>
                                        <label for="NameofProposedSociety">3. Name of Proposed Society</label>
                                    </div>
                                    <div class="col-md-6 form-group">
                                        @Html.DropDownList("ClassofSocietyandLiability", new SelectList(ViewBag.ClassOfSociety as System.Collections.IEnumerable, "Value", "Text"),
                                        "Select", new { id = "ClassofSocietyandLiability", @class = "form-control", required = "required" })
                                        <label for="ClassofSocietyandLiability">4. Class of Society and Liability</label>
                                    </div>
                                    <div class="col-md-6 form-group">
                                        @Html.DropDownList("SubClassOfSociety", new SelectList(Enumerable.Empty<SelectListItem>(), "SocietySubClassCode", "SocietySubClassName"),
                                        "Select", new { id = "SubClassOfSociety", @class = "form-control", required = "required" })
