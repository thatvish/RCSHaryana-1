                    <form class="form form-validate" novalidate="novalidate" action="@Url.Action("Application", "Society", new { value = TempData["Value"]})" accept-charset="utf-8" method="post">
                        <div class="row">
                            <div class="card">
                                <div class="card-body">
                                    <div class="col-md-6 form-group">
                                        @Html.DropDownList("District", new SelectList(ViewBag.District as System.Collections.IEnumerable, "Value", "Text"),
