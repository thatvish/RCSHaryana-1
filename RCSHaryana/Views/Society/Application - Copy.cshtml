                                            </div>
                                            <div class="col-md-6 form-group">
                                                <input type="text" class="form-control" id="@($"RegisteredAddress{i}")" name="@($"RegisteredAddress{i}")" required>
                                                <label for="RegisteredAddress+@i">11. Place of Residence</label>
                                            </div>
                                            <div class="col-md-6 form-group">
                                                <input type="text" class="form-control" id="@($"HouseNoSectorNoRoad{i}")" name="@($"HouseNoSectorNoRoad{i}")" required>
                                                <label for="HouseNoSectorNoRoad+@i">11.1 House No. /Sector No. /Road</label>
                                            </div>
                                            <div class="col-md-6 form-group">
                                                <input type="text" class="form-control" id="@($"PostOffice{i}")" name="@($"PostOffice{i}")" required>
                                                <label for="PostOffice+@i">11.2 Post Office</label>
                                            </div>
                                            <div class="col-md-6 form-group">
                                                <input type="text" class="form-control" id="@($"PostalCode{i}")" name="@($"PostalCode{i}")" data-rule-number="true" maxlength="6" minlength="6" pattern="[0-9]{6}" data-rule-digits="true" required>
