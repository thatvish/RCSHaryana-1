    function CheckUserNameExist(frgtValue) {
        var dataToSend = frgtValue;
        sessionStorage.setItem("frgtval", dataToSend);
        if (dataToSend == "")
        {
            $("#validOTP").css("display", "block");
            $("#validOTP").html("<span style='color:#B22222'>UserName is Mandatory</span>");
            alert("Please Enter User Name First");
            return false;
        }
        $.ajax({
            url: "/Account/CheckExistUserName",
            type: "GET",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            data: { dataToSend },
            success: function (result) {
                var QuestionId = result.QuestionId;
                if (QuestionId == 0) {
                    alert("UserName doesn't Exist. Please Enter Correct UserName.");
                    $("#validOTP").css("display", "block");
                    $("#validOTP").html("<span style='color:#B22222'>Invalid UserName</span>");
                    $('#frgtusername').val('');
                    return false;
                }
                if (QuestionId == 1) {
                    var q1 = "What is the last name of the teacher who gave you your first failing grade?";
                }
                else if (QuestionId == 2) {
                    var q1 = "What was the name of your elementary / primary school?";
                }
                else if (QuestionId == 3) {
                    var q1 = "In what city or town does your nearest sibling live?";
                }
                document.getElementById('labelchangable').innerHTML = q1;
                $('.btnTextChange').val("Submit Answer");
                $('#frgtusername').css('display', 'none');
                $('#enterPinValue').css('display', 'none');
                $('#enterSecurityAns').css('display', 'block');
                $("#validOTP").css("display", "none");
            },
            error: function (errormessage) {
            }
        });
    }
    function CheckSecurityAns(securityAns) {
        if (securityAns == "") {
            alert("Please Enter Answer First");
            $("#validOTP").css("display", "block");
            $("#validOTP").html("<span style='color:#B22222'>Answer is Mandatory</span>");
            return false;
        }
        var dataToSend = sessionStorage.getItem("frgtval");
        $.ajax({
            url: "/Account/checkSecurityAns",
            type: "GET",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            data: { dataToSend, securityAns },
            success: function (result) {
                var QuestionAns = result.QuestionAns;
                var MobileNo = result.MobileNo;
                if (QuestionAns == "NotMatch") {
                    alert("Security Answer doesn't Match. Please Enter Correct Answer.");
                    $("#validOTP").css("display", "block");
                    $("#validOTP").html("<span style='color:#B22222'>Incorrect Answer</span>");
                    $('#enterSecurityAns').val('');
                    $('#enterPinValue').val('');
                    $('#frgtConfirmPassword').val('');
                    $('#frgtPassword').val('');
                    return false;
                }
                if (QuestionAns == "SomethingWrong") {
                    alert("Something Went Wrong.Try again later");
                    $('#enterSecurityAns').val('');
                    $("#validOTP").css("display", "none");
                    $('#frgtusername').val('');
                    $('#enterPinValue').val('');
                    $('#frgtConfirmPassword').val('');
                    $('#frgtPassword').val('');
                      window.location.href = '@Url.Action("LogIn", "Account")';
                }
                $('#enterSecurityAns').val('')
                var getLast = MobileNo;
                var lastThree = getLast.substr(getLast.length - 3);
                $('#labelchangable').html("OTP has been sent to your Registered Mobile No. " + "*******" + lastThree);
                $('#enterSecurityAns').css("display", "none");
                $("#validOTP").css("display", "none");
                $('#enterPinValue').css('display', 'block');
                $('#mycounter').css('display', 'block');
                $('.btnTextChange').val("Submit Pin");
                onTimer();
            },
            error: function (errormessage) {
            }
        });
    }
