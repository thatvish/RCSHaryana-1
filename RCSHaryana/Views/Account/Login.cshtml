    function FrgtencrptUsername(Username) {
        var value = $("#frgtusername").val();
        if (value.length >= 1) {
            var url = "/Account/GetUserNameEncrpted";
            var UserName = $('#frgtusername').val();
            $.get(url, function (data) {
                $('#frgtusername').val(hex_hmac_md5(data, value));
            });
        }
        else {
            return false;
        }
    }
    $(window).load(function () {
        $('#username').val('');
    });

    $('#password').on("keypress", function (e) {
        if (e.keyCode == 13) {
            var value = $("#password").val();
            if (value.length >= 1) {
                var url = "/Account/EncrptPWD";
                $.get(url, function (data) {
                    $('#password').val(hex_hmac_md5(data, value))
                });
            }
        }
    });
</script>
