@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>RCS - Login</title>
    <!-- BEGIN META -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="your,keywords">
    <meta name="description" content="Short explanation about this website">
    <!-- END META -->
    <!-- BEGIN STYLESHEETS -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:300italic,400italic,300,400,500,700,900' rel='stylesheet' type='text/css' />
    <link type="text/css" rel="stylesheet" href="/content/assets/css/theme-default/bootstrap.css?1422792965" />
    <link type="text/css" rel="stylesheet" href="/content/assets/css/theme-default/materialadmin.css?1425466319" />
    <link type="text/css" rel="stylesheet" href="/content/assets/css/theme-default/font-awesome.min.css?1422529194" />
    <link type="text/css" rel="stylesheet" href="/content/assets/css/theme-default/material-design-iconic-font.min.css?1421434286" />
    <!-- END STYLESHEETS -->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    @*[if lt IE 9]>
        <script type="text/javascript" src="/content/assets/js/libs/utils/html5shiv.js?1403934957"></script>
        <script type="text/javascript" src="/content/assets/js/libs/utils/respond.min.js?1403934956"></script>
        <![endif]*@
    <style type="text/css">
        .form-group.captcha-reply {
            margin: 0 !important;
            padding: 0 !important;
        }

        .section.section-account {
            padding: 0 !important;
        }

        .captcha-reply img#CaptchaImage {
            width: 125px;
        }

        .captcha-reply input#CaptchaInputText {
            width: 100%;
            background: transparent;
            border: none;
            border-bottom: 1px solid;
            border-color: rgba(12, 12, 12, 0.12);
        }
    </style>
</head>
<body class="menubar-hoverable header-fixed ">
    <!-- BEGIN LOGIN SECTION -->
    <section class="section-account" style="padding: 0;">

        <div class="container card style-transparent">
            <div class="card-body" style="padding: 0;">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 cus-outer-div">
                    <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12 cus-slider-div">
                        <div class="carousel fade-carousel slide" data-ride="carousel" data-interval="6000" id="bs-carousel">
                            <!-- Overlay -->
                            <div class="overlay"></div>
                            <!-- Indicators -->
                            <ol class="carousel-indicators">
                                <li data-target="#bs-carousel" data-slide-to="0" class="active"></li>
                                <li data-target="#bs-carousel" data-slide-to="1"></li>
                                <li data-target="#bs-carousel" data-slide-to="2"></li>
                            </ol>

                            <!-- Wrapper for slides -->
                            <div class="carousel-inner">
                                <div class="item slides active">
                                    <div class="slide-1"></div>
                                    <div class="hero cus-slider-content">
                                        <hgroup>
                                            <h1>CO-OPERATION DEPARTMENT</h1>
                                            <h2>Government of Haryana</h2>
                                            <h2 class="cus-sep"></h2>
                                            <h3>The Objective and the role of Co-operation Department is to see that genuine Co-operatives are organized and managed on sound lines as per basic principles of Co-operation for the promotion of the economic interests, social betterment and welfare of the people and to render guidance and assistance for the development in the state.</h3>
                                        </hgroup>

                                    </div>
                                </div>
                                <div class="item slides">
                                    <div class="slide-2"></div>
                                </div>
                                <div class="item slides">
                                    <div class="slide-3"></div>
                                    <div class="hero text-center cus-slider-content" style="padding: 20px;">
                                        <hgroup>
                                            <h1>CO-OPERATION DEPARTMENT</h1>
                                            <h2>Government of Haryana</h2>
                                            <h2 class="cus-sep"></h2>
                                            <h3 style="text-align:center;">Don't have an account yet ?</h3>
                                        </hgroup>
                                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center cus-signup-bttn">
                                            <a class="btn btn-block btn-raised btn-primary cus-singup-bttn" href="~/Account/Registration">Click to Sign Up</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 cus-login-div">
                        <h3>Sign In to continue</h3>
                        <form class="form floating-label" action="@Url.Action("Login", "Account")" accept-charset="utf-8" method="post">
                            @Html.AntiForgeryToken()
                            <div class="form-group">
                                <input type="text" class="form-control" id="username" name="username" required>
                                <label for="username">Username</label>
                            </div>
                            <div class="form-group">
                                <input type="password" class="form-control" id="password" name="password" required>
                                <label for="password">Password</label>
                                @*<p class="help-block"><a href="#">Forgotten?</a></p>*@
                            </div>
                            <div id="captcha_content2">
                                @{Html.RenderPartial("_Captcha");}
                                <p style="font-weight:bold;color:red;">@ViewBag.ErrMessage</p>
                            </div>
                            @*<div class="form-group">
                                   <p id="math-captcha"></p>
                                </div>

                                <div class="form-group">
                                    <label for="password"><img src="/Home/ShowCaptchaImage" title="Captcha" /></label>
                                    <br />
                                    <input type="text" name="clientCaptcha" class="form-control" placeholder="Write above text exactly" required />
                                    <br />
                                    <p style="font-weight:bold;color:red;">@ViewBag.CaptchaError</p>
                                </div>*@

                            <div class="row">
                                <div class="col-xs-6 text-left">
                                    <div class="checkbox checkbox-inline checkbox-styled">
                                        <label>
                                            <input type="checkbox"><span>Remember me</span>
                                        </label>
                                    </div>
                                </div><!--end .col -->
                                <div class="col-xs-6 text-right">
                                    <button class="btn btn-primary btn-raised" type="submit">Login</button>
                                </div><!--end .col -->
                            </div><!--end .row -->
                        </form>
                        <h3 class="text-light cus-no-acc">
                            No account yet?
                        </h3>
                        <a class="btn btn-block btn-raised btn-primary cus-singup-bttn" href="~/Account/Registration">Sign up here</a>
                    </div><!--end .col -->
                </div><!--end .row -->
            </div><!--end .card-body -->
        </div><!--end .card -->
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 cus-footer">
            <p>website is Designed and Developed by <span style="color: #ff6000;">NIC Haryana</span></p>
        </div>
    </section>
    <!-- END LOGIN SECTION -->
    <!-- BEGIN JAVASCRIPT -->
    <script src="/content/assets/js/libs/jquery/jquery-1.11.2.min.js"></script>
    <script src="/content/assets/js/libs/jquery/jquery-migrate-1.2.1.min.js"></script>
    <script src="/content/assets/js/libs/bootstrap/bootstrap.min.js"></script>
    <script src="/content/assets/js/libs/spin.js/spin.min.js"></script>
    <script src="/content/assets/js/libs/autosize/jquery.autosize.min.js"></script>
    <script src="/content/assets/js/libs/nanoscroller/jquery.nanoscroller.min.js"></script>
    <script src="/content/assets/js/core/source/App.js"></script>
    <script src="/content/assets/js/core/source/AppNavigation.js"></script>
    <script src="/content/assets/js/core/source/AppOffcanvas.js"></script>
    <script src="/content/assets/js/core/source/AppCard.js"></script>
    <script src="/content/assets/js/core/source/AppForm.js"></script>
    <script src="/content/assets/js/core/source/AppNavSearch.js"></script>
    <script src="/content/assets/js/core/source/AppVendor.js"></script>
    <script src="/content/assets/js/core/demo/Demo.js"></script>
    <script src="~/scripts/custom.js"></script>
    <script src="https://www.google.com/recaptcha/api.js" async defer></script>
    <!-- END JAVASCRIPT -->

</body>
</html>