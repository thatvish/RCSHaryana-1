    var i = 60;
    function onTimer() {
        document.getElementById('mycounter').innerHTML = "OTP will be expired in " + i +" second";
        $("#mycounter").css("color","white");

        i--;
        if (i < 0) {
            alert('OTP has been expired. Try again');
            window.location.href = '@Url.Action("LogIn", "Account")';
        }
        else {
            var timer = setTimeout(onTimer, 1000);
            sessionStorage.setItem("timerVal", timer);
        }
    }
    function myStopFunction() {
        var timerVal = sessionStorage.getItem("timerVal");
        clearTimeout(timerVal);
    }
