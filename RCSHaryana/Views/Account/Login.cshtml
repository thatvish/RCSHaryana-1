    function ForgetcheckPasswordMatch() {
        var frgtpassword = $("#frgtPassword").val();
        var frgtconfirmPassword = $("#frgtConfirmPassword").val();
        var frgtConfirmPasswordLength = $("#frgtConfirmPassword").val().length;
        var frgtPasswordLength = $("#frgtPassword").val().length;
        if (frgtpassword == frgtconfirmPassword & frgtConfirmPasswordLength >= 8 & frgtPasswordLength >= 8) {
            $("#frgtdivCheckPasswordMatch").html("<span style='color:#228B22'>Password match.</span>");
            $("#frgtresultPassword").html("<span style='color:#228B22'>Password match.</span>");
            document.getElementById("submitBtnId1").disabled = false;
            $("#frgtinfo").html("<span></span>");
            $("#frgtdivPassword").addClass("has-success has-feedback");
            $("#frgtpasswordstatus").addClass("glyphicon glyphicon-ok form-control-feedback");
            $("#frgtdivConfirmPassword").addClass("has-success has-feedback");
            $("#frgtConfirmPasswordstatus").addClass("glyphicon glyphicon-ok form-control-feedback");

            var url = "/Account/encrptPWD";
            $.get(url, function (data) {
                $('#frgtPassword').val(hex_hmac_md5(data, frgtpassword))
                $('#frgtConfirmPassword').val(hex_hmac_md5(data, frgtconfirmPassword))
                $('#frgtPassword').removeAttr("pattern");
                $('#frgtConfirmPassword').removeAttr("pattern");
            });

        }
        else {
            if (frgtConfirmPasswordLength >= 8 & frgtPasswordLength >= 8) {
                $("#frgtdivCheckPasswordMatch").html("<span style='color:#B22222'>Passwords do not match!</span>");
                $("#frgtresultPassword").html("<span style='color:#B22222'>Passwords do not match!</span>");
                document.getElementById("submitBtnId1").disabled = true;
                $("#frgtinfo").html("<span style='color:#B22222'>create button disable because password not matched.</span>");
                $("#frgtdivPassword").removeClass();
                $("#frgtdivPassword").addClass("has-error has-feedback");
                $("#frgtpasswordstatus").removeClass();
                $("#frgtdivConfirmPassword").removeClass();
                $("#frgtdivConfirmPassword").addClass("has-error has-feedback");
                $("#frgtConfirmPasswordstatus").removeClass();

            }
            else {
                document.getElementById("submitBtnId1").disabled = true;
                $("#frgtdivCheckPasswordMatch").html("<span style='color:#B22222'>Passwords do not match!</span>");
                $("#frgtresultPassword").html("<span style='color:#B22222'>Passwords do not match!</span>");
                $("#frgtinfo").html("<span style='color:#B22222'>password should be 8 char long</span>");
                $("#frgtdivPassword").removeClass();
                $("#frgtdivPassword").addClass("has-error has-feedback");
                $("#frgtpasswordstatus").removeClass();
                $("#frgtdivConfirmPassword").removeClass();
                $("#frgtdivConfirmPassword").addClass("has-error has-feedback");
                $("#frgtConfirmPasswordstatus").removeClass();
            }
        }
    }
