<script src="~/scripts/js/ListOfManagingCommitteeMembersForOfficers.js"></script>
<section>
    <div class="section-header">
        <ol class="breadcrumb">
            <li class="active">Approved Society</li>
        </ol>
    </div>
    <div class="section-body contain-lg">
        <div class="card tabs-left style-default-light">
            <!-- BEGIN SEARCH BAR -->
            <div class="card-body style-primary no-y-padding">
                <form class="form form-inverse" action="@Url.Action("ApprovedSociety", "ARCS")" accept-charset="utf-8" method="post">
                    <div class="form-group">
                        <div class="input-group input-group-lg">
                            <div class="input-group-content">
                                <div class="form-control-line"></div>
                                @Html.DropDownList("SocietyList", new SelectList(ViewBag.lstSL as System.Collections.IEnumerable, "Value", "Text"),
                                        "Select", new { id = "SocietyList", @class = "form-control", required = "required" })
                            </div>
                            <div class="input-group-btn">
                                <button id="btnSearch" class="btn btn-floating-action btn-default-bright" type="submit"><i class="fa fa-search"></i></button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table no-margin">
                                <thead>
                                    <tr>
                                        <th>
                                            Society Serial No
                                        </th>
                                        <th>
                                            Society Name
                                        </th>
                                        <th>
                                            Action
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>@ViewBag.SocietyTransID</td>
                                        <td>@ViewBag.SocietyName</td>
                                       @if(!string.IsNullOrEmpty(Convert.ToString(Session["SocietyTransID"])))
                                        {
                                            <td>
                                                <a href="@Url.Action("Registrationcertificate", "ARCS")" class="btn btn btn-primary">View Ceriticate</a>
                                            </td>
                                        }  
                                    </tr>
                                </tbody>
                            </table>
                        </div><!--end .table-responsive -->
                    </div><!--end .card-body -->
                </div><!--end .card -->
            </div>
        </div>
    </div>
</section>
<script>
    $(document).ready(function () {
        var number = parseInt(@ViewBag.tabResult); //Accessing the number from the ViewBag
        if (number != "NaN") {
            if (number == 5) {
                alert("Hello");
                $('#mytabs a[href="#Forward"]').tab('show');
            }
        }
    });
</script>
<script language="javascript" type="text/javascript">
    function noBack() { window.history.forward() }
    noBack();
    window.onload = noBack;
    window.onpageshow = function (evt) { if (evt.persisted) noBack() }
    window.onunload = function () { void (0) }
</script>
<script type="text/javascript" language="Javascript">
    history.go(1);
</script>
